{
  "name": "secureweb",
  "displayName": "Secureweb",
  "version": "0.0.1",
  "description": "Secure Web is a tool which detect malicious url's and secure user from various attack's",
  "author": "techbuddies",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.89.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "<all_urls>"
    ],
    "permissions": [
      "identity",
      "tabs",
      "activeTab",
      "scripting",
      "notifications",
      "unlimitedStorage"
    ]
  }
}
